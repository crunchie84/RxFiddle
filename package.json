    "chai": "^3.5.0",
    "electron": "^1.4.4",
    "mocha": "^3.1.2",
    "mocha-typescript": "^1.0.10",
    "source-map-support": "^0.4.5",
    "tsify": "^1.0.9",
    "tslint": "^3.15.1",
    "watchify": "^3.7.0"
  }
