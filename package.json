    "@types/node": "^6.0.45",
    "chai-shallow-deep-equal": "^1.4.4",
    "dagre": "^0.7.4",
    "error-stack-parser": "^1.3.6",
    "esprima": "^3.1.3",
    "graphlib": "1.0.7",
    "immutable": "^3.8.1",
    "lodash": "4.17.2",
    "rx": "^4.1.0",
    "snabbdom": "0.6.5",
    "svg-pan-zoom": "github:ariutta/svg-pan-zoom",
