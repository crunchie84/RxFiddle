    "bundle": "browserify dist/src/main.js -o dist/main.bundle.js && browserify dist/src/editor.js -o dist/editor.bundle.js",
    "test": "tsc -p . && mocha --require source-map-support/register dist/test",
    "dev-test-watch": "mocha-typescript-watch",
    "prepublish": "tsc -p ."
  },
  "dependencies": {
    "@types/dagre": "^0.7.33",
    "@types/electron": "^1.4.24",
